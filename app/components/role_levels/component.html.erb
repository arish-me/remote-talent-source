<div>
  <h2 class="text-sm font-medium text-gray-500"><%= t(".interested_in_roles") %></h2>
  <ul class="mt-3 space-y-3">
    <% role_levels.each do |role_level| %>

        <li class="flex items-center space-x-2">
          <%= inline_svg_tag "icons/solid/check_circle.svg", class: "h-5 w-5 text-gray-400", aria_hidden: true %>
          <span class="text-gray-900 text-sm font-medium"><%= role_level.name.humanize %></span>
        </li>

    <% end %>
  </ul>
</div>
