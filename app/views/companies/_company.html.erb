<div id="<%= dom_id company %>">
  <p>
    <strong>User:</strong>
    <%= company.user_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= company.name %>
  </p>

  <p>
    <strong>Company email:</strong>
    <%= company.company_email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= company.phone %>
  </p>

  <p>
    <strong>Website:</strong>
    <%= company.website %>
  </p>

  <p>
    <strong>Size:</strong>
    <%= company.size %>
  </p>

  <p>
    <strong>Industry:</strong>
    <%= company.industry %>
  </p>

</div>
